
{{# each contents }}
<tr>
   <td>{{ this.name }}</td>
   <td>{{ this.email }}</td>
   <td>{{ this.nationality }}</td>
   <td>{{ this.company }}</td>
   
   <td>
      <a href="/update-sponsor?id={{this.id}}" class="btn border-shadow update"> 
        <span class="text-gradient"><i class="fa fa-pencil" style="color:blue; font-size:20px;"></i></span>
      </a>
      <a href="api/sponsor/{{this._id}}" class="btn border-shadow delete">
        <span class="text-gradient"><i class="fa fa-trash"  style="color:red; font-size:20px;"></i></span>
      </a>
    </td>
</tr>
{{/each}}

